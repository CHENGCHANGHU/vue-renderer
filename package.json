{
  "name": "@golden-tiger/vue-renderer",
  "version": "0.0.3",
  "description": "@golden-tiger/vue-renderer",
  "main": "./lib/cjs-module.js",
  "module": "./lib/es-module.js",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "dev:mac": "mode=development minimize=false webpack --config webpack.config.js --env development --env minimize=false",
    "build:mac": "mode=production minimize=true webpack --config webpack.config.js --env production --env minimize",
    "dev:windows": "set mode=development&&set minimize=false&&webpack --config webpack.config.js --env development --env minimize=false",
    "build:windows": "set mode=production&&set minimize=true&&webpack --config webpack.config.js --env production --env minimize",
    "publish:patch": "node ./publish-helper.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CHENGCHANGHU/vue-renderer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "golden-tiger",
    "vue-renderer"
  ],
  "author": "Cheng ChangHu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CHENGCHANGHU/vue-renderer/issues"
  },
  "homepage": "https://github.com/CHENGCHANGHU/vue-renderer#readme",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.25.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@golden-tiger/regexp-gene": "^0.0.6",
    "vue": "^3.3.4"
  }
